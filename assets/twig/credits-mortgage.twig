<meta name="viewport" content="width=device-width,minimum-scale=1.0'">
       {% include 'content/includes/header.twig' %}
    <main>
     <section class="container credits-list-container">
         <div class="row">
             <div class="col-xs-12">
                  {% for i,row in position3 %}
                        {{ row|raw }}
                    {% endfor %}
             </div>
         </div>
         <div class="row">
             <div class="row-height">
                {% for i,row in position4 %}
                     {{ row|raw }}
                {% endfor %}
             </div>
         </div>
     </section>
        
     <!-- PARALLAX IMG -->
        {% include 'content/includes/parallax2.twig'%}

     <section>
        <article class="credits-article">
            {% for i,row in position5 %}
                     {{ row|raw }}
                {% endfor %}
         </article>
         <article class="credits-article">
             {% for i,row in position7 %}
                     {{ row|raw }}
                {% endfor %}
         </article>

         <article class="credits-article">
            {% for i,row in position8 %}
                    {{ row|raw }}
            {% endfor %} 
         </article>
         <article class="credits-article">
              {% for i,row in position9 %}
                    {{ row|raw }}
            {% endfor %}
         </article>
     </section>
     {% include 'content/includes/head.twig' %}
    </main>
    
    {% include 'content/includes/footer.twig' %}
    <script type="text/javascript" src="public/adamska/js/jquery.rd-parallax.js"></script>
    <script src="public/adamska/js/mobile-menu.js"></script>
    <script src="public/adamska/js/jquery.easing.1.3.min.js"></script>
    <script>
    $(document).ready(function() {
        $('a.page-scroll').bind('click', function(event) {
            var $anchor = $(this);
            $('html, body').stop().animate({
                scrollTop: $($anchor.attr('href')).offset().top
            }, 1500, 'easeInOutCubic');
            event.preventDefault();
        });
    });
    </script>